{
  "contentVersion": "1.0.0",
  "polite_masu": {
    "label": "Polite (Masu-Form)",
    "description": "Standard polite present/future affirmative.",
    "introducedIn": "N5.5",
    "rules": {
      "godan": {
        "type": "godan_change",
        "map": "u_to_i",
        "add": "ます"
      },
      "ichidan": {
        "type": "suffix",
        "remove": "る",
        "add": "ます"
      },
      "irr_suru": {
        "type": "replace",
        "surface": "します",
        "reading": "します"
      },
      "irr_kuru": {
        "type": "replace",
        "surface": "来ます",
        "reading": "きます"
      },
      "irr_aru": {
        "type": "replace",
        "surface": "あります",
        "reading": "あります"
      }
    }
  },
